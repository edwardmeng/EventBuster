    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Transactions" targetFramework="net3.5"/>
      <frameworkAssembly assemblyName="System.Transactions" targetFramework="net4.5.1"/>
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="lib\**\*.dll" target="lib" />
    <file src="lib\**\*.xml" target="lib" />
    <file src="lib\**\*.deps.json" target="lib" />
    <file src="..\LICENSE" />
